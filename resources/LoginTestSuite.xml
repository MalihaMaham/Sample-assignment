<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="PositiveLoginTestSuite" verbose="1" parallel="tests" thread-count="4">
    <test name="UserLoginTest-CHROME">
        <parameter name="username" value="user@phptravels.com"></parameter>
        <parameter name="password" value="demouser"></parameter>
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="au.com.beginnerseleniumframework.Tests.PositiveLoginTest">
            </class>
        </classes>
    </test>
    <test name="AgentLoginTest-CHROME">
        <parameter name="username" value="agent@phptravels.com"></parameter>
        <parameter name="password" value="demoagent"></parameter>
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="au.com.beginnerseleniumframework.Tests.PositiveLoginTest">
            </class>
        </classes>
    </test>
    <test name="UserLoginTest-FIREFOX">
        <parameter name="username" value="user@phptravels.com"></parameter>
        <parameter name="password" value="demouser"></parameter>
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="au.com.beginnerseleniumframework.Tests.PositiveLoginTest">
            </class>
        </classes>
    </test>
    <test name="AgentLoginTest-FIREFOX-HEADLESS">
        <parameter name="username" value="agent@phptravels.com"></parameter>
        <parameter name="password" value="demoagent"></parameter>
        <parameter name="browser" value="firefoxheadless"></parameter>
        <classes>
            <class name="au.com.beginnerseleniumframework.Tests.PositiveLoginTest">
            </class>
        </classes>
    </test>
</suite>